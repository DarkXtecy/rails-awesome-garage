<h1>All Cars</h1>
<ul>
  <% @cars.each do |car| %>
    <li>
      <strong><%= car.brand %> <%= car.model %></strong>
      (<%= car.year %>, <%= car.fuel %>)
      - Owned by: <%= car.owner.nickname %>
      <br>
      <%= link_to "View Details", car_path(car) %>
    </li>
  <% end %>
</ul>
<p>
  <%= link_to "My Garage", favourites_path %>
</p>
